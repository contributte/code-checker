{
  "name": "contributte/code-checker",
  "description": "nette/code-checker optimized for co-operation with our coding standard",
  "keywords": [
    "nette",
    "code checker",
    "coding standard"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/code-checker",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">=7.2",
    "nette/code-checker": "^3.1.0"
  },
  "require-dev": {
    "ninjify/qa": "^0.12",
    "phpstan/phpstan": "^0.12",
    "phpstan/phpstan-deprecation-rules": "^0.12",
    "phpstan/phpstan-strict-rules": "^0.12"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\CodeChecker\\": "src/"
    }
  },
  "bin": [
    "bin/checker"
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.2.x-dev"
    }
  }
}
