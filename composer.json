{
  "name": "contributte/code-checker",
  "description": "nette/code-checker optimized for co-operation with our coding standard",
  "keywords": [
    "nette",
    "code checker",
    "coding standard"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/code-checker",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">=8.1",
    "nette/code-checker": "^3.3.2"
  },
  "require-dev": {
    "contributte/qa": "^0.4",
    "contributte/tester": "^0.3",
    "contributte/phpstan": "^0.1"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\CodeChecker\\": "src/"
    }
  },
  "bin": [
    "bin/checker"
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.3.x-dev"
    }
  }
}
